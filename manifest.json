{
    "manifest_version": 2,
    "name": "a-rin-tile",
    "version": "0.1",
    "description": "新規タブに、あーりんの画像を敷き詰めます",
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' https://api.tumblr.com; object-src 'self'",
	"icons": {
		"16": "img/16.png",
		"48": "img/48.png",
		"128": "img/128.png"
	},
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "permissions": [
        "https://api.tumblr.com/"
    ]
}
